{
  "name": "bundlecraft-frontend",
  "version": "1.0.0",
  "description": "BundleCraft - Turn Printify Products Into Profitable Bundles",
  "homepage": "https://bundlecraft.netlify.app",
  "main": "src/App.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:analyze": "npm run build && npx serve -s build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "dev": "react-scripts start",
    "deploy": "npm run build && netlify deploy --prod",
    "lint": "eslint src --ext js,jsx",
    "format": "prettier --write src/**/*.{js,jsx,json,css}",
    "tailwind:build": "tailwindcss build src/styles/tailwind.css -o src/styles/tailwind.generated.css"
  },
  "keywords": [
    "react",
    "printify",
    "etsy",
    "ecommerce",
    "saas",
    "bundles"
  ],
  "author": "BundleCraft Team",
  "license": "ISC",
  "dependencies": {
    "@stripe/react-stripe-js": "^2.1.0",
    "@stripe/stripe-js": "^3.1.0",
    "axios": "^1.6.8",
    "framer-motion": "^10.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.4.1",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
 
